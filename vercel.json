{
    "builds":[
        {
            "src":"./backend/index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/.*",
            "dest":"/backend/index.js"
        },
        {
            "src":"/var/task/frontend/src/logs/",
            "dest":"/frontend/src/logs/"
        },
        {
            "src":"/var/task/frontend/src/uploads/",
            "dest":"/frontend/src/uploads/"
        }
    ]
}